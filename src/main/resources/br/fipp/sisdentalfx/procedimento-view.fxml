<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.fipp.sisdentalfx.ProcedimentoViewController">
   <children>
      <Label text="ID">
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <TextField fx:id="tfID" editable="false">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Label layoutX="30.0" layoutY="106.0" text="Descrição">
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <TextField fx:id="tfDescricao" layoutX="30.0" layoutY="127.0">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Label layoutX="30.0" layoutY="162.0" text="Tempo">
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <TextField fx:id="tfTempo" layoutX="30.0" layoutY="183.0">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Label text="Valor">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <TextField fx:id="tfValor">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <HBox alignment="CENTER" prefHeight="48.0" prefWidth="560.0" spacing="40.0">
         <children>
            <Button fx:id="btConfirmar" mnemonicParsing="false" prefWidth="90.0" style="-fx-background-color: lightblue;" text="Confirmar">
               <font>
                  <Font size="14.0" />
               </font></Button>
            <Button fx:id="btCancelar" mnemonicParsing="false" prefWidth="90.0" style="-fx-background-color: salmon;" text="Cancelar">
               <font>
                  <Font size="14.0" />
               </font></Button>
         </children>
         <VBox.margin>
            <Insets top="15.0" />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="20.0" right="20.0" top="10.0" />
   </padding>
</VBox>
